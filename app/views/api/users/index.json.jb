@users.map do |user|
  {
    first_name: user.first_name,
    last_name: user.last_name,
    email: user.email,
    birth_date: user.birth_date,
    height: user.height,
    weight: user.weight,
    username: user.user_name,
    password: user.password,
    address: user.address,
    gym: user.business,
  }

end